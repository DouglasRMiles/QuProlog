export BASES.psi	= $(basename $(SOURCES.psi))
export OBJECTS.psi	= $(addsuffix .o,$(BASES.psi))

export GENERATED.psi = pseudo_instr_arrays.cc

.PHONY:	all.psi
all.psi: $(LIBRARY.psi)

$(LIBRARY.psi): $(OBJECTS.psi)
	$(AR) $(ARFLAGS) $@ $(OBJECTS.psi)
	@RANLIB@ $@


.PHONY: install.psi
install.psi: all.psi

.PHONY:	clean.psi
clean.psi:
	$(RM) $(OBJECTS.psi) $(LIBRARY.psi)



